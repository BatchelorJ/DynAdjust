{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) debug import",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dynadjust/build_gcc/dynadjust/dnaimportwrapper/dnaimport",
            "args": ["-n driver", "dsg.stn", "dsg-pseudo-driver-file.msr", "--simulate", "--geo-file", "dsg.geo", "--export-dna"],
            "stopAtEntry": true,
            "cwd": "/home/roger/dist",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) debug reftran",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dynadjust/build_gcc/dynadjust/dnareftranwrapper/dnareftran",
            "args": ["50k", "-r itrf2014"],
            "stopAtEntry": true,
            "cwd": "/home/roger/dist",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) debug adjust",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dynadjust/build_gcc/dynadjust/dnaadjustwrapper/dnaadjust",
            "args": ["dsg", "--phased", "--output-adj-msr", "--output-ignored"],
            "stopAtEntry": false,
            "cwd": "/home/roger/dist",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) debug segment",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dynadjust/build_gcc/dynadjust/dnasegmentwrapper/dnasegment",
            "args": ["dsg", "--min 1", "--max 2"],
            "stopAtEntry": true,
            "cwd": "/home/roger/dist",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
